{
  "private": true,
  "name": "toster-tags-parser",
  "version": "0.0.1",
  "description": "",
  "author": "Yarkov Aleksey",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:toster-extension/toster-tags-parser.git"
  },
  "bugs": "https://github.com/toster-extension/toster-tags-parser/issues",
  "engines": {
    "node": ">= 9.0.0",
    "npm": "^5.5.1"
  },
  "bin": {
    "toster-tags-parser": "bin/toster-tags-parser"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.ts",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.ts --watch",
    "lint": "tslint -c tslint.json -p tsconfig.json --force",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "cli-progress": "^2.1.1",
    "minimist": "^1.2.0",
    "mkdirp-sync": "^0.0.3",
    "puppeteer": "^1.12.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^1.8.0",
    "@types/minimist": "^1.2.0",
    "@types/puppeteer": "^1.12.1",
    "@types/rollup": "^0.54.0",
    "cross-env": "^5.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.4.0",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript": "^1.0.0",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.3.1",
    "typescript": "^3.3.3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint:fix",
      "git add"
    ]
  }
}
